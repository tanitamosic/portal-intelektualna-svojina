PREFIX pred: <http://www.xmlsux.com/predicate/>
PREFIX xsd: <http://www.w3.org/2001/XMLSchema#>

SELECT (xsd:string(?broj_prijave) AS ?prijave)
FROM <http://localhost:8081/fuseki/Metadata/data/P1/Metadata>
WHERE {
   <pred:formaZaZavod> pred:brojPrijave ?broj_prijave .
   <pred:formaPodnosioca> pred:vrstaPrijave <%vrsta_prijave>^^<http://www.w3.org/1999/02/22-rdf-syntax-ns#XMLLiteral> .
}